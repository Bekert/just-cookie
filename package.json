{
    "name": "just-cookie",
    "version": "0.1.0",
    "description": "Simply library for manage cookie",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/Bekert/JustCookie.git",
    "author": "Bekert <bekert@tutanota.com>",
    "license": "MIT",
    "devDependencies": {
        "cz-customizable": "^6.2.0",
        "nodemon": "^2.0.4",
        "@types/node": "^14.0.5",
        "typescript": "^3.9.3"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
